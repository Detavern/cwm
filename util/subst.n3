 "$Id: subst.n3,v 1.2 2002-10-02 15:57:45 connolly Exp $".

@prefix log: <http://www.w3.org/2000/10/swap/log#>.

@prefix : <#>.

this log:forAll :X, :Y, :Z, :P.

{ :X :P :Y. :X = :Z.
  :P log:notEqualTo log:forAll, log:forSome
} log:implies
 { :Z :P :Y }.

{ :X :P :Y. :P = :Q.
  :P log:notEqualTo log:forAll, log:forSome
} log:implies { :X :Q :Y }.

{ :X :P :Y. :Y = :Z.
  :P log:notEqualTo log:forAll, log:forSome
} log:implies { :X :P :Z }.


